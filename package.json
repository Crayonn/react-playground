{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint src",
    "build:dev:client": "webpack --config config/webpack.config.dev.js",
    "build:dev:server": "webpack --config config/webpack.config.server.js",
    "build:dev": "npm run clean && npm run build:dev:server && npm run build:dev:client",
    "build:prod:client": "NODE_ENV=production webpack --config config/webpack.config.prod.js -p",
    "build:prod:server": "NODE_ENV=production webpack --config config/webpack.config.server.js",
    "prebuild": "npm run clean",
    "build": "npm run build:prod:server && npm run build:prod:client",
    "prestart:release": "npm run build",
    "start:release": "node build/server.js",
    "prestart": "npm run clean",
    "start": "NODE_ENV=development DEBUG=react-playground:* node scripts/dev.js",
    "dev": "NODE_ENV=development DEBUG=react-playground:* node scripts/dev.js",
    "test": "NODE_ENV=test NODE_PATH=src ava",
    "test:watch": "npm run test -- --watch"
  },
  "ava": {
    "files": [
      "tests/**/*.spec.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "require": [
      "./tests/setup.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlebedynskyi/react-playground.git"
  },
  "license": "MIT",
  "author": "Dima Lebedynskyi",
  "homepage": "https://github.com/dlebedynskyi/react-playground",
  "dependencies": {
    "body-parser": "^1.16.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "helmet": "^3.4.0",
    "hpp": "^0.2.1",
    "ignore-styles": "^5.0.1",
    "react": "^15.4.2",
    "react-async-component": "^0.2.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "4.0.0-beta.6",
    "react-router-dom": "^4.0.0-beta.5",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "ava": "^0.18.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalkline": "0.0.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-module-utils": "^2.0.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "opn": "^4.0.2",
    "postcss": "^5.2.0",
    "postcss-loader": "^1.1.1",
    "postcss-smart-import": "^0.6.7",
    "precss": "^1.4.0",
    "react-dev-utils": "^0.5.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
